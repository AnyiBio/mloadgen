{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "OrderPublicDetailDocument",
  "type": "object",
  "description": "Contains information about items and their shipping, status, payments...",
  "properties": {
    "_class": {
      "type": "string",
      "description": "Non-functional attribute. Java class associated.",
      "enum": [
        "OrderPublicDetailDocument"
      ]
    },
    "_entity": {
      "type": "string",
      "description": "Non-functional attribute. Identifies the entity described by the document.",
      "enum": [
        "OrderPublicDetailDocument"
      ]
    },
    "_metadata": {
      "type": "object",
      "description": "Non-functional attribute. Metadata associated with the document (audit data).",
      "properties": {
        "createdAt": {
          "type": "number",
          "description": "Datetime on which the document is created. (UTC)",
          "minimum": 0,
          "unit": "Timestamp"
        },
        "createdBy": {
          "type": "string",
          "description": "User that creates the document."
        },
        "deletedAt": {
          "type": "number",
          "description": "Datetime on which the document was deleted. (UTC)",
          "minimum": 0,
          "unit": "Timestamp"
        },
        "deletedBy": {
          "type": "string",
          "description": "User that deleted the document."
        },
        "lastUpdatedAt": {
          "type": "number",
          "description": "Datetime on which the last update was performed. (UTC)",
          "minimum": 0,
          "unit": "Timestamp"
        },
        "lastUpdatedBy": {
          "type": "string",
          "description": "User that performed the last update to the document."
        },
        "projectName": {
          "type": "string",
          "description": "Project name"
        },
        "projectVersion": {
          "type": "string",
          "description": "Project version"
        },
        "schema": {
          "type": "number",
          "description": ""
        }
      },
      "required": [
        "createdAt",
        "createdBy",
        "lastUpdatedAt",
        "lastUpdatedBy"
      ],
      "additionalProperties": false
    },
    "orderId": {
      "type": "string",
      "description": "Public identifier of the order",
      "default": "",
      "examples": [
        "f3a57b4f-7a59-4f52-9c82-180028115aa5"
      ],
      "pattern": "^(.*)$",
      "x-metadata": {
        "binding": {
          "entity": "Cliente",
          "attribute": "numeroPedidoOnline",
          "version": "1.0.0"
        },
        "sensitivity": "RESTRINGIDA"
      }
    },
    "storeId": {
      "type": "integer",
      "description": "Virtual store where the order was placed",
      "default": 0,
      "examples": [
        123456
      ]
    },
    "paymentId": {
      "type": "string",
      "description": "Generated transaction in the payments system, if any",
      "default": "",
      "examples": [
        "1db24a06"
      ],
      "pattern": "^(.*)$"
    },
    "parentId": {
      "type": "string",
      "description": "In a exchange or replacement order, the origin orderId",
      "default": "",
      "examples": [
        "00f505de2c74"
      ],
      "pattern": "^(.*)$"
    },
    "externalId": {
      "type": "string",
      "description": "In a order from an external origin (Tmall, instagram...), the id witch the origin system refers to order",
      "default": "",
      "examples": [
        "2f40dc7e17c8"
      ],
      "pattern": "^(.*)$"
    },
    "editorId": {
      "type": "integer",
      "description": "Identifier of the agent who is editing the order",
      "default": 0,
      "examples": [
        123456
      ]
    },
    "type": {
      "type": "string",
      "description": "Type of the order that indicates if the order is a normal order or an exchange/replacement",
      "enum": [
        "REGULAR",
        "REPLACEMENT",
        "EXCHANGE",
        "REPOSITION",
        "ONLINE_EXCHANGE",
        "STORE_EXCHANGE"
      ]
    },
    "status": {
      "type": "string",
      "description": "General status of the order",
      "enum": [
        "RECEIVED",
        "VALIDATED",
        "PREPARING",
        "SHIPPED",
        "CLOSED",
        "CANCELLED"
      ]
    },
    "fraudStatus": {
      "type": "string",
      "description": "Status of fraud review process",
      "enum": [
        "NOT_APPLIED",
        "PENDING",
        "ACCEPTED",
        "REQUIRED_REVIEW",
        "REJECTED"
      ]
    },
    "stockStatus": {
      "type": "string",
      "description": "Status of stock system",
      "enum": [
        "UNKNOWN",
        "NOT_APPLIED",
        "SYNCHRONIZED",
        "SKIPPED"
      ]
    },
    "paymentStatus": {
      "type": "string",
      "description": "Status of the order payment",
      "enum": [
        "NOT_APPLIED",
        "UNKNOWN",
        "REJECTED",
        "ACCEPTED"
      ]
    },
    "paymentSystem": {
      "type": "string",
      "description": "System that has managed the payment of the order",
      "enum": [
        "UNKNOWN",
        "ALPHA",
        "WCS"
      ]
    },
    "manualReviewStatus": {
      "type": "string",
      "description": "Status of manual review for the order",
      "enum": [
        "NA",
        "PENDING",
        "ACCEPTED",
        "REFUSED"
      ]
    },
    "customerId": {
      "type": "integer",
      "description": "Customer that placed the order",
      "default": 0,
      "examples": [
        123444
      ]
    },
    "customerType": {
      "type": "string",
      "description": "Indicates if the customer is registered or guest",
      "enum": [
        "REGISTERED",
        "GUEST"
      ]
    },
    "guestAction": {
      "type": "string",
      "description": "Indicates if the guest client who made the order performed any action with their account at the end of the order (CONVERSION / LINK ACCOUNT)",
      "enum": [
        "NO_ACTION",
        "CONVERSION",
        "LINK_ACCOUNT"
      ]
    },
    "locale": {
      "type": "string",
      "description": "Language in which the customer has placed the order",
      "default": "",
      "examples": [
        "es_ES"
      ],
      "pattern": "^(.*)$"
    },
    "preorder": {
      "type": "boolean",
      "description": "Is the order a preorder?",
      "default": false,
      "examples": [
        true
      ]
    },
    "fastSint": {
      "type": "boolean",
      "description": "Indicates if the order has been processed in fastSINT mode",
      "default": false,
      "examples": [
        true
      ]
    },
    "pvpTaxesIncluded": {
      "type": "boolean",
      "description": "Does the order must be shown with taxes included?",
      "default": false,
      "examples": [
        true
      ]
    },
    "placedDatetime": {
      "type": "string",
      "description": "The moment the order was placed (UTC)",
      "default": "",
      "examples": [
        "2019-12-03T10:15:30Z"
      ],
      "pattern": "^(.*)$"
    },
    "cancelledDatetime": {
      "type": "string",
      "description": "The moment the order was cancelled (if it was cancelled) (UTC)",
      "default": "",
      "examples": [
        "2019-12-04T10:15:30Z"
      ],
      "pattern": "^(.*)$"
    },
    "lastUpdateDatetime": {
      "type": "string",
      "description": "The moment the order had its last update (UTC)",
      "examples": [
        "2019-12-03T10:15:30Z"
      ],
      "pattern": "^(.*)$"
    },
    "snapshotDatetime": {
      "type": "string",
      "description": "The moment the order was placed (UTC)",
      "examples": [
        "2019-12-03T10:15:30Z"
      ],
      "pattern": "^(.*)$"
    },
    "snapshotTrigger": {
      "type": "string",
      "description": "Name of the event that triggered the snapshot",
      "enum": [
        "ORDER_CREATED",
        "REPAYMENT_DONE",
        "REPAYMENT_NEEDED",
        "PAYMENT_CONFIRMED",
        "PAYMENT_UPDATED",
        "PAYMENT_SYNC",
        "PAYMENT_CLOSED",
        "FRAUD_VALIDATED",
        "FRAUD_TO_REVIEW",
        "IN_MANUAL_REVIEW",
        "IN_WAREHOUSE",
        "STOCKOUT",
        "IN_EDITION",
        "FORWARD_ORDER",
        "BILLING_ADDRESS_UPDATED",
        "SHIPPING_ADDRESS_UPDATED",
        "ORDER_CANCELLED",
        "BILLING_UPDATED",
        "BILLING_THREAD",
        "EXPEDITION",
        "ORDER_TRACKING",
        "ON_DEMAND",
        "SIMPA",
        "DELIVERY_INFO_UPDATED",
        "DISTRIBUTION_UPDATED",
        "ORDER_PARTIALLY_CANCELLED",
        "CUSTOMER_UPDATED",
        "CHECKOUT_COMPLETED"
      ]
    },
    "duty": {
      "type": "object",
      "description": "Info about the amounts of duty for the order",
      "properties": {
        "amount": {
          "type": "object",
          "description": "Customs duty amount",
          "required": [
            "value",
            "currency",
            "exponent"
          ],
          "properties": {
            "value": {
              "type": "integer",
              "description": "Value of the amount in currency's minor units",
              "default": 0,
              "examples": [
                1000
              ]
            },
            "currency": {
              "type": "string",
              "description": "Currency code of the amount according ISO 4217",
              "default": "",
              "examples": [
                "EUR"
              ],
              "pattern": "^(.*)$"
            },
            "exponent": {
              "type": "integer",
              "description": "Indicates the 10 exponent of the amount.",
              "default": 0,
              "examples": [
                2
              ]
            }
          }
        },
        "confirmedAmount": {
          "type": "object",
          "description": "Customs confirmed duty amount",
          "required": [
            "value",
            "currency",
            "exponent"
          ],
          "properties": {
            "value": {
              "type": "integer",
              "description": "Value of the amount in currency's minor units",
              "default": 0,
              "examples": [
                900
              ]
            },
            "currency": {
              "type": "string",
              "description": "Currency code of the amount according ISO 4217",
              "default": "",
              "examples": [
                "EUR"
              ],
              "pattern": "^(.*)$"
            },
            "exponent": {
              "type": "integer",
              "description": "Indicates the 10 exponent of the amount.",
              "default": 0,
              "examples": [
                2
              ]
            }
          }
        }
      }
    },
    "billing": {
      "type": "object",
      "description": "Info about the billing of the order",
      "required": [
        "status",
        "addressId"
      ],
      "properties": {
        "status": {
          "type": "string",
          "description": "Status of the order billing",
          "enum": [
            "NOT_APPLIED",
            "PENDING",
            "BILLED"
          ]
        },
        "addressId": {
          "type": "string",
          "description": "Billing address",
          "default": "",
          "examples": [
            "44138f25"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "origin": {
      "type": "object",
      "description": "Info about the billing of the order",
      "required": [
        "systemType"
      ],
      "properties": {
        "systemCode": {
          "type": "string",
          "description": "Code of the system that places the order",
          "default": "",
          "examples": [
            "a_systemCode"
          ],
          "pattern": "^(.*)$"
        },
        "systemType": {
          "type": "string",
          "description": "System that places the order (Checkout, MPS, BO, STORE)",
          "enum": [
            "CHECKOUT",
            "MPS",
            "BACKOFFICE",
            "STORE"
          ]
        },
        "systemUser": {
          "type": "string",
          "description": "Identifier of the user who creates the order (Store Employee / BO User)",
          "default": "",
          "examples": [
            "1db24a06"
          ],
          "pattern": "^(.*)$"
        },
        "systemDeviceId": {
          "type": "string",
          "description": "Identifier of the serial number of the device from which the checkout was made (only for store)",
          "default": "",
          "examples": [
            "1db24a06"
          ],
          "pattern": "^(.*)$"
        },
        "deviceType": {
          "type": "string",
          "description": "Device (browser, mobile, iphone ...)",
          "default": "",
          "examples": [
            "mobile"
          ],
          "pattern": "^(.*)$",
          "x-metadata": {
            "binding": {
              "entity": "Cliente",
              "attribute": "dispositivo",
              "version": "1.0.0"
            },
            "sensitivity": "RESTRINGIDA"
          }
        },
        "deviceUserAgent": {
          "type": "string",
          "description": "Contains information about web browser name, operating system, device type and lots of other information.",
          "default": "",
          "examples": [
            "ZaraApp/8.15.3 (iPhone; iOS 13.5.1) (compatible; Zara/App/v8.15.3)"
          ],
          "pattern": "^(.*)$",
          "x-metadata": {
            "binding": {
              "entity": "Cliente",
              "attribute": "navegador",
              "version": "1.0.0"
            },
            "sensitivity": "RESTRINGIDA"
          }
        }
      }
    },
    "orderItems": {
      "type": "array",
      "description": "List of items included in the order",
      "items": {
        "type": "object",
        "required": [
          "id",
          "type",
          "pvpAmount",
          "promotions"
        ],
        "properties": {
          "id": {
            "type": "string",
            "description": "Public identifier of the item",
            "default": "",
            "examples": [
              "d011c1f751bf"
            ],
            "pattern": "^(.*)$"
          },
          "partNumber": {
            "type": "string",
            "description": "Identifier of the product that represent the item",
            "default": "",
            "examples": [
              "1814229826d1"
            ],
            "pattern": "^(.*)$"
          },
          "type": {
            "type": "string",
            "description": "Indicates if the item is a product, gift card or shipping costs",
            "enum": [
              "PRODUCT",
              "VIRTUAL_GIFT_CARD",
              "PHYSICAL_GIFT_CARD",
              "SHIPPING_COST",
              "SERVICE"
            ]
          },
          "status": {
            "type": "string",
            "description": "Status of the item",
            "enum": [
              "RECEIVED",
              "PREPARING",
              "SHIPPED",
              "CLOSED",
              "CANCELLED"
            ]
          },
          "subStatus": {
            "type": "string",
            "description": "SubStatus of the item",
            "enum": [
              "RECEIVED",
              "SENT_FFC",
              "IN_FFC",
              "STOCKOUT",
              "RESERVED_INTO_STOCKOUT",
              "UNDER_REVIEW",
              "NEW",
              "EXPEDITED",
              "IN_COURIER",
              "IN_STORE",
              "IN_DROPPOINT",
              "DELIVERY_ATTEMPTED",
              "DELIVERED_TO_CUSTOMER",
              "DELIVERED_IN_DROPPOINT",
              "REJECTED",
              "DELIVERY_OMISSION",
              "MISSED",
              "EXPIRED",
              "CANCELLED"
            ]
          },
          "statusReason": {
            "type": "string",
            "description": "Reason why the item is in that state",
            "default": "",
            "pattern": "^(.*)$"
          },
          "statusDatetime": {
            "type": "string",
            "description": "Date when item was in this status (UTC)",
            "default": "",
            "examples": [
              "2019-11-06T10:15:30Z"
            ],
            "pattern": "^(.*)$"
          },
          "pvpAmount": {
            "type": "object",
            "description": "Price of the item with shown to the customer",
            "required": [
              "value",
              "currency",
              "exponent"
            ],
            "properties": {
              "value": {
                "type": "integer",
                "description": "Value of the amount in currency's minor units",
                "default": 0,
                "examples": [
                  400
                ]
              },
              "currency": {
                "type": "string",
                "description": "Currency code of the amount according ISO 4217",
                "default": "",
                "examples": [
                  "EUR"
                ],
                "pattern": "^(.*)$"
              },
              "exponent": {
                "type": "integer",
                "description": "Indicates the 10 exponent of the amount.",
                "default": 0,
                "examples": [
                  2
                ]
              }
            }
          },
          "taxesAmount": {
            "type": "object",
            "description": "Taxes of the item",
            "required": [
              "value",
              "currency",
              "exponent"
            ],
            "properties": {
              "value": {
                "type": "integer",
                "description": "Value of the amount in currency's minor units",
                "default": 0,
                "examples": [
                  600
                ]
              },
              "currency": {
                "type": "string",
                "description": "Currency code of the amount according ISO 4217",
                "examples": [
                  "EUR"
                ],
                "pattern": "^(.*)$"
              },
              "exponent": {
                "type": "integer",
                "description": "Indicates the 10 exponent of the amount.",
                "default": 0,
                "examples": [
                  2
                ]
              }
            }
          },
          "taxesPercentage": {
            "type": "number",
            "description": "Taxes percentage that apply to the item",
            "default": 0.0,
            "examples": [
              10.4
            ]
          },
          "initialMinimumDate": {
            "type": "string",
            "description": "Date when the order will arrive as minimum (UTC)",
            "default": "",
            "examples": [
              "2019-11-06T10:15:30Z"
            ],
            "pattern": "^(.*)$"
          },
          "initialMaximumDate": {
            "type": "string",
            "description": "Date when the order will arrive as maximum (UTC)",
            "default": "",
            "examples": [
              "2019-11-06T10:15:30Z"
            ],
            "pattern": "^(.*)$"
          },
          "adjustment": {
            "type": "object",
            "description": "Total amount of applied adjustments",
            "properties": {
              "value": {
                "type": "integer",
                "description": "Value of the amount in currency's minor units",
                "default": 0,
                "examples": [
                  400
                ]
              },
              "currency": {
                "type": "string",
                "description": "Identifier of the currency used in all amounts of the order",
                "default": "",
                "examples": [
                  "EUR"
                ],
                "pattern": "^(.*)$"
              },
              "exponent": {
                "type": "integer",
                "description": "Number of decimal places that applies to all amounts in the order. It is given by the store",
                "default": 0,
                "examples": [
                  2
                ]
              }
            }
          },
          "product": {
            "type": "object",
            "description": "Set of info relative to the item with type 'product'",
            "required": [
              "catalogEntryId",
              "stockMode"
            ],
            "properties": {
              "catalogEntryId": {
                "type": "integer",
                "description": "Identifier of the product",
                "default": 0,
                "examples": [
                  1234
                ]
              },
              "stockMode": {
                "type": "string",
                "description": "Indicates if the product exists as stock or is a preorder",
                "enum": [
                  "UNKNOWN",
                  "NORMAL",
                  "TRANSIT_PRESALE",
                  "VIRTUALSTOCK_PRESALE",
                  "SOD_PRESALE",
                  "PREORDER"
                ]
              },
              "xmedia": {
                "type": "array",
                "description": "Set of info about media elements associated to the product",
                "items": {
                  "type": "object",
                  "properties": {
                    "xmediaSet": {
                      "type": "integer",
                      "description": "A product may have more than one shooting session. Each session is a different set",
                      "default": 0,
                      "examples": [
                        6
                      ]
                    },
                    "type": {
                      "type": "string",
                      "description": "Type of media: image, video, etc. Used to determine the file format and extension",
                      "default": "",
                      "examples": [
                        "a_type"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "kind": {
                      "type": "string",
                      "description": "Kind of image: plain, full, etc",
                      "default": "",
                      "examples": [
                        "a_kind"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "path": {
                      "type": "string",
                      "description": "Path over the static content URL where the media is located.",
                      "default": "",
                      "examples": [
                        "a_path"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "name": {
                      "type": "string",
                      "description": "Name of the file. Is used to generate the url to the media.",
                      "default": "",
                      "examples": [
                        "a_name"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "width": {
                      "type": "integer",
                      "description": "Max width of the media in full quality.",
                      "default": 0,
                      "examples": [
                        10
                      ]
                    },
                    "height": {
                      "type": "integer",
                      "description": "Max height of the media in full quality.",
                      "default": 0,
                      "examples": [
                        20
                      ]
                    },
                    "timestamp": {
                      "type": "string",
                      "description": "Timestamp that indicates when the image was changed last time. It is used to generate the media URL and bypass cache every time it changes.",
                      "default": "",
                      "examples": [
                        "2019-12-06T10:15:30Z"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "allowedScreens": {
                      "type": "array",
                      "description": "Screen sizes suitable for this media: small, large.",
                      "items": {
                        "type": "string",
                        "default": "",
                        "examples": [
                          "an_allowedScreen",
                          "another_allowedScreen"
                        ],
                        "pattern": "^(.*)$"
                      }
                    },
                    "locations": {
                      "type": "array",
                      "description": "Locations selected to use this media on.",
                      "items": {
                        "type": "object",
                        "required": [
                          "name",
                          "order"
                        ],
                        "properties": {
                          "name": {
                            "type": "string",
                            "description": "Name of the location: CATEGORY_PAGE, PRODUCT_PAGE, SHOPCART_PAGE, MINI_SHOPCART, RELATED_PRODUCT, BUNDLE_COMPONENT, etc.",
                            "default": "",
                            "examples": [
                              "location_name"
                            ],
                            "pattern": "^(.*)$"
                          },
                          "order": {
                            "type": "integer",
                            "description": "Indicates the precedence of medias for an specific location.",
                            "default": 0,
                            "examples": [
                              12
                            ]
                          }
                        }
                      }
                    }
                  }
                }
              },
              "customizations": {
                "type": "array",
                "description": "List of customizations applied to the product",
                "items": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string",
                      "description": "Customer selection. Right now, only RTF is allowed",
                      "default": "",
                      "examples": [
                        "a_type"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "text": {
                      "type": "string",
                      "description": "Customer selection",
                      "default": "",
                      "examples": [
                        "a_text"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "typography": {
                      "type": "string",
                      "description": "Customer selection",
                      "default": "",
                      "examples": [
                        "a_typography"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "color": {
                      "type": "string",
                      "description": "Customer selection",
                      "default": "",
                      "examples": [
                        "a_color"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "area": {
                      "type": "string",
                      "description": "Customer selection",
                      "default": "",
                      "examples": [
                        "a_area"
                      ],
                      "pattern": "^(.*)$"
                    }
                  }
                }
              }
            }
          },
          "virtualGiftCard": {
            "type": "object",
            "description": "Set of info relative to the item with type 'virtual gift card'",
            "required": [
              "catalogEntryId"
            ],
            "properties": {
              "catalogEntryId": {
                "type": "integer",
                "description": "Identifier of the product",
                "default": 0,
                "examples": [
                  234561
                ]
